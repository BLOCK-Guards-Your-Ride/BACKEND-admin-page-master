{
  "name": "block-angular",
  "version": "0.0.0",
  "sasslintConfig": "./sass-lint.yml",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:dev": "ng serve --configuration=dev",
    "serve:tst": "ng serve --configuration=tst",
    "build": "ng build",
    "test": "ng test",
    "lint": "tslint --project tslint.json",
    "e2e": "ng e2e",
    "sass-lint": "sass-lint -c sass-lint.yml -v -q",
    "build-dev": "ng build --configuration=dev",
    "build-tst": "ng build --configuration=tst",
    "build-prd": "ng build --configuration=production",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "npm run sass-lint && npm run lint"
    }
  },
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@angular/animations": "^9.1.13",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.13",
    "@angular/compiler": "^9.1.13",
    "@angular/core": "^9.1.13",
    "@angular/forms": "^9.1.13",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.13",
    "@angular/platform-browser-dynamic": "^9.1.13",
    "@angular/router": "^9.1.13",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "event-source-polyfill": "^1.0.25",
    "eventsource": "^1.1.0",
    "js-sha256": "^0.9.0",
    "keycloak-angular": "^8.4.0",
    "keycloak-js": "^15.0.2",
    "lodash": "^4.17.21",
    "ngx-cookie-service": "^12.0.3",
    "ngx-countdown": "^12.0.1",
    "rxjs": "^6.6.7",
    "tslib": "^1.14.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.15",
    "@angular/cli": "^9.1.15",
    "@angular/compiler-cli": "^9.1.13",
    "@angular/language-service": "^9.1.13",
    "@types/jasmine": "^2.8.18",
    "@types/jasminewd2": "^2.0.10",
    "@types/lodash": "^4.14.174",
    "codelyzer": "^5.2.2",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.4",
    "sass-lint": "^1.13.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "^3.7.7"
  }
}
